{
  "name": "mmmf",
  "version": "1.1.0",
  "description": "MMMF - Max Money Market Funds. Balance forecasting tool.",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "vite --config client/vite.config.js",
    "build": "vite build --config client/vite.config.js",
    "preview": "vite preview --config client/vite.config.js",
    "docker:build": "docker build -t mmmf .",
    "docker:run": "docker run -p 5173:5173 -v ./data:/app/data mmmf",
    "deploy:worker": "wrangler deploy",
    "deploy:pages": "wrangler pages deploy dist",
    "preview:cf": "npm run build && wrangler pages dev dist"
  },
  "keywords": [
    "mmmf",
    "budget",
    "forecast",
    "accounting",
    "balance",
    "money-market-funds"
  ],
  "author": "Jason Yang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jasonyang-ee/MMMF.git"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.4.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^5.2.1",
    "hono": "^4.11.4",
    "nanoid": "^5.1.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "postcss": "^8.5.6",
    "react": "^19.2.3",
    "react-datepicker": "^8.10.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.2.3",
    "vite": "^7.2.6",
    "wrangler": "^4.53.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
