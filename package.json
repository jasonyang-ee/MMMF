{
  "name": "mmmf",
  "version": "1.0.7",
  "description": "MMMF - Max Money Market Funds",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "docker:build": "docker build -t balance-forecast .",
    "docker:run": "docker run -p 3600:3600 -v ./data:/app/data balance-forecast",
    "preview:cf": "npm run build && wrangler pages dev dist",
    "deploy:worker": "wrangler deploy",
    "deploy:pages": "wrangler pages deploy dist"
  },
  "keywords": [
    "mmmf",
    "budget",
    "forecast",
    "accounting",
    "balance"
  ],
  "author": "Jason Yang",
  "license": "MIT",
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.4.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^5.2.1",
    "hono": "^4.11.1",
    "nanoid": "^5.1.6"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "postcss": "^8.5.6",
    "react": "^19.2.3",
    "react-datepicker": "^8.10.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.2.3",
    "@tailwindcss/postcss": "^4.1.17",
    "vite": "^7.2.6",
    "wrangler": "^4.53.0"
  }
}
